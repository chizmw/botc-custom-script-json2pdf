{
  "postcssSorting.config": {
    "properties-order": "alphabetical",
    "order": [
      "custom-properties",
      "dollar-variables",
      "declarations",
      "at-rules",
      "rules"
    ]
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python"
}
