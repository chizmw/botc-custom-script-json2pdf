<!-- Maybe Jinxes On The Night Info Sheet-->
{%- if hate_pair %}
  <div class="line-prejinx-nightinfo"></div>
  <h2 class="character-type character-type-jinx">{{ "jinxes" | upper }}</h2>
  <div class="cols-2">
  {%- for tag, reason in hate_pair.items() %}
      <div class="jinx-reminder-block">
        {%- set slugs = tag.split("-") %}
        {%- for slug in slugs %}
          <img src="file://{{icon_folder}}/{{slug}}.png" alt="{{ reason }}" class="jinx-night-icon"/>
        {%- endfor %}
        <span class="jinx-night-reason">{{ reason }}</span>
      </div>
  {%- endfor %}
  </div>
{%- endif %}
